<body>

         <a href="/signout">    <button type="submit" class="btn btn-danger">Sign Out</button></a>

  <br><br>

  <section>

    <div class="container">
      <div class="row">
        {{#each items}}
        <div class="col-md-3">
          <div class="card" style="width: 18rem;">
            <img src="{{this.image_url}}" style="height: 300px;" class="card-img-top" alt="...">
            <div class="card-body">
              <h5 class="card-title">{{this.name}}</h5>
              <p class="card-text">{{this.declaration}}</p>
              <a href="#" class="btn btn-primary">Add CArt</a>
              <h5>{{this.price}}</h5>
              <h5>{{this.category}}</h5>

            </div>
          </div>
        </div>
        {{/each}}
      </div>
    </div>
  </section>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
    crossorigin="anonymous"></script>
</body>

</html>